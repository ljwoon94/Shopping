<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shopping.mapper.BoardMapper">
	<insert id="create">
		INSERT INTO board(
			title,
			content,
			writer
		)
		VALUES(
			#{title},
			#{content},
			#{writer}
		)
	</insert>
</mapper>